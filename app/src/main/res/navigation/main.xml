<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    app:startDestination="@id/signInFragment">


    <fragment
        android:id="@+id/signInFragment"
        android:name="com.android.tikalarcorefuse.login.SignInFragment"
        android:label="SignInFragment" >
        <action
            android:id="@+id/action_signInFragment_to_createRoom"
            app:destination="@id/createRoom" />
    </fragment>


    <fragment
        android:id="@+id/createRoom"
        android:name="com.android.tikalarcorefuse.createroom.CreateRoom"
        android:label="CreateRoom" />

</navigation>